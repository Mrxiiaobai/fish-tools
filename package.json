{
  "name": "fish-tools",
  "displayName": "Fish Tools",
  "description": "摸鱼专用，超大屏看热搜",
  "icon": "./resources/dark/fish.png",
  "version": "1.0.4",
  "license": "ISC",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "fish-activityBar": [
        {
          "id": "fishView",
          "name": "fish weibo"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "fish-activityBar",
          "title": "Fish Weibo",
          "icon": "resources/dark/weibo.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "fishView.refreshEntry",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "fishView.refreshEntry",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "FishTools",
      "properties": {
        "fishTools.interval": {
          "type": "number",
          "default": 30,
          "markdownDescription": "单位分钟，刷新间隔，默认30分钟，修改后即时生效"
        },
        "fishTools.cookie": {
          "type": "string",
          "default": "",
          "markdownDescription": "cookie，必填，请修改后刷新",
          "editPresentation": "multilineText"
        }
      }
    }
  },
  "scripts": {
    "watch": "npm-run-all -p watch:*",
    "watch:extension": "tsc -watch -p ./",
    "watch:views": "webpack --watch --mode development",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "gulp package",
    "build": "vsce package",
    "publish": "vsce publish",
    "compile": "npm-run-all compile:*",
    "compile:extension": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
    "compile:views": "webpack --mode production"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "antd": "^5.10.1",
    "css-loader": "^6.8.1",
    "esbuild": "^0.19.5",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "mocha": "^10.1.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^7.3.3",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.0",
    "typescript": "^4.9.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "fs": "^0.0.1-security",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Mrxiiaobai/auto-tools.git"
  },
  "publisher": "mrsavior",
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo \"[Husky] pre-commit\"",
      "commit-msg": "commitlint",
      "pre-push": "npm run changelog"
    }
  }
}